{
  "name": "source-parser",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "start": "ts-node-dev ./src/index.ts -P tsconfig.json --no-cache",
    "build": "tsc -P tsconfig.json",
    "test": "jest --config jestconfig.json --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "codecov": "codecov"
  },
  "hooks": {
    "pre-commit": "npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Corki418/source-parser.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Corki418/source-parser/issues"
  },
  "homepage": "https://github.com/Corki418/source-parser#readme",
  "dependencies": {
    "@types/jest": "^26.0.5",
    "@types/node": "^14.0.23",
    "codecov": "^3.7.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node-dev": "^1.0.0-pre.52",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  }
}
